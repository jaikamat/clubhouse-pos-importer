{
    "name": "monolith",
    "version": "0.0.0",
    "private": true,
    "scripts": {
        "start": "node ./built/bin/www",
        "gcp-build": "tsc -p .",
        "build": "rm -r ./built && tsc -p . && echo \"Built TS source\"",
        "start-dev": "npm run build && PORT=7331 node ./built/bin/www",
